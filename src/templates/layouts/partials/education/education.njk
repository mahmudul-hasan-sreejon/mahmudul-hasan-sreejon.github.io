      <!-- education section start -->
      <section class="resume-section p-3 p-lg-5 d-flex flex-column" id="education">
        <div class="my-auto">
  
          <h2 class="mb-5">Education</h2>
  
          {% for education in educationSection %}
          <div class="resume-item d-flex flex-column flex-md-row mb-5">
            <div class="resume-content mr-auto">
              <h3 class="mb-0">{{education.institute}}</h3>
              <div class="subheading mb-3">{{education.certificate}}</div>
              <div>{{education.department}}</div>
              <!-- <p>{{education.result}}</p> -->
            </div>
  
            <div class="resume-date text-md-right">
              <b><span class="text-primary">{{education.duration}}</span></b>
            </div>
          </div>
          {% endfor %}
        </div>
      </section>
      <!-- education section end -->