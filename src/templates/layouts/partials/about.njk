      <!-- about section start -->
      <section class="resume-section p-3 p-lg-5 d-flex d-column" id="about">
        <div class="my-auto">
          
          <!-- about section user info start -->
          <h1 class="mb-0" style="font-size:75px">{{personalInfo.name.first}} {{personalInfo.name.last}} <span class="text-primary">{{personalInfo.name.pet}}</span></h1>
  
          <div class="subheading mb-5">{{personalInfo.address}} | {{personalInfo.phoneNumber}} |
            <a href="mailto:{{personalInfo.email}}" style="text-decoration:none">{{personalInfo.email}}</a>
          </div>
          <!-- about section user info end -->
  
  
          <!-- about section user overview start -->
          <ul class="fa-ul lead mb-5">
            {% for overview in aboutSection.userOverview %}
            <li>
              <i class="fa-li fa fa-angle-double-right text-warning"></i>
              {{overview}}
            </li>
            {% endfor %}
          </ul>
          <!-- about section user overview start -->
  
  
          <!-- about section user contact start -->
          <div class="social-icons">
            {% for contact in aboutSection.userContact %}
            <a href="{{contact.href}}" class="mb-1 new-tab-open">
              <i class="{{contact.icon}}"></i>
            </a>
            {% endfor %}
          </div>
          <!-- about section user contact start -->
  
        </div>
      </section>
      <!-- about section end -->